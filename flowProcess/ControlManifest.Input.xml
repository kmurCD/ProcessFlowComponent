<?xml version="1.0" encoding="utf-8"?>
<manifest>
  <control namespace="Transligra" constructor="FlowProcess" version="0.0.9" display-name-key="flowProcess" description-key="flowProcess description" control-type="virtual">
    <external-service-usage enabled="false">
      <!-- Dominios externos si son necesarios -->
    </external-service-usage>
    
    <!-- Propiedades principales -->
    <property name="phase" display-name-key="Phase" of-type="OptionSet" usage="bound" required="true" />
    <property name="controlphase" display-name-key="ControlPhase" of-type="Whole.None" usage="bound" required="true" />
    <property name="userSettings" display-name-key="User Settings" of-type="SingleLine.Text" usage="bound" required="false"/>

    <!--1 Prospección-->
    <!--2 Calificacion de la oportunidad-->
    <property name="parentcontactid" display-name-key="Contacto" of-type="Lookup.Simple" usage="bound" required="false" />
    <property name="proyectoComprometido" display-name-key="¿Proyecto comprometido?" of-type="TwoOptions" usage="bound" required="false" />
    <property name="presupuestoDefinidoEstimado" display-name-key="Presupuesto Definido/Estimado" of-type="Currency" usage="bound" required="false" />
    <property name="procesoCompra" display-name-key="Proceso de compra" of-type="OptionSet" usage="bound" required="false" />
    <property name="actividadescalificacion" display-name-key="Actividades Calificación Oportunidad" of-type="SingleLine.TextArea" usage="bound" required="false" />

    <!-- Fase 3: Levantamiento Tecnico -->
    <property name="alcanceServiciosDefinido" display-name-key="Alcance servicios definido" of-type="TwoOptions" usage="bound" required="false" />
    <property name="cantidadDeUsuarios" display-name-key="Cantidad de usuarios" of-type="Whole.None" usage="bound" required="false" />
    <property name="actividadeslevantamiento" display-name-key="Actividades Levantamiento" of-type="SingleLine.Text" usage="bound" required="false" />
    <property name="documentoArquitectura" display-name-key="Documento de Arquitectura" of-type="SingleLine.Text" usage="bound" required="false" />
   
    <!-- Fase 4: Elaboración de la Propuesta Técnica -->
    <property name="fechaEstimadaPresentacionPropuesta" display-name-key="Fecha estimada presentación propuesta" of-type="DateAndTime.DateOnly" usage="bound" required="false" />
    <property name="entendimientoPropuesta" display-name-key="Entendimiento de la Propuesta Técnica" of-type="OptionSet" usage="bound" required="false" />
    <property name="documentoPropuestaTecnica" display-name-key="Documento Propuesta Técnica" of-type="SingleLine.Text" usage="bound" required="false" />

    <!-- Fase 5: Presentación de Propuesta Técnica -->
    <property name="fechaAprobacionPropuesta" display-name-key="Fecha aprobación propuesta técnica" of-type="DateAndTime.DateOnly" usage="bound" required="false" />
    <property name="evaluacionClientePropuesta" display-name-key="Evaluación cliente propuesta técnica" of-type="OptionSet" usage="bound" required="false" />
   
    <!-- Fase 6: Elaboración de la Propuesta Económica -->
    <property name="fechaobjetivopropuestaeconomica" display-name-key="Fecha objetivo propuesta económica" of-type="DateAndTime.DateOnly" usage="bound" required="false" />
    <property name="precioObjetivo" display-name-key="Precio objetivo" of-type="Currency" usage="bound" required="false" />
    <property name="estrategiaPrecios" display-name-key="Estrategia de precios" of-type="OptionSet" usage="bound" required="false" />
   
    <!-- Fase 7: Propuesta enviada al cliente -->
    <property name="fechaEnvio" display-name-key="Fecha de envío" of-type="DateAndTime.DateOnly" usage="bound" required="false" />
    <property name="fechaSeguimiento" display-name-key="Fecha de seguimiento" of-type="DateAndTime.DateOnly" usage="bound" required="false" />
    <property name="montoRevenue" display-name-key="Monto Revenue" of-type="Currency" usage="bound" required="false" />
    <property name="montoProfit" display-name-key="Monto Profit" of-type="Currency" usage="bound" required="false" />

    <!-- Fase 8: Negociación -->
    <property name="fechaEstimadaCierre" display-name-key="Fecha estimada de cierre" of-type="DateAndTime.DateOnly" usage="bound" required="false" />
    <property name="fechaEstimadaFacturacion" display-name-key="Fecha estimada de facturación" of-type="DateAndTime.DateOnly" usage="bound" required="false" />
    <property name="brechasNegociacion" display-name-key="Brechas de negociación" of-type="OptionSet" usage="bound" required="false" />
    <property name="variablesNegociacion" display-name-key="Variables de negociación" of-type="OptionSet" usage="bound" required="false" />
   
    <!-- Fase 9: Cierre Comercial -->
    <property name="tipodedocumento" display-name-key="Tipo de documento" of-type="OptionSet" usage="bound" required="false" />
    <property name="fechaTransferencia" display-name-key="Fecha transferencia Preventa a Postventa" of-type="DateAndTime.DateOnly" usage="bound" required="false" />
    <property name="fechaKickOff" display-name-key="Fecha de kick-off" of-type="DateAndTime.DateOnly" usage="bound" required="false" />
    <property name="equipoTrabajoDefinido" display-name-key="Equipo de trabajo definido" of-type="TwoOptions" usage="bound" required="false" />
   
    <!-- Fase 10: Formalización -->
    <property name="fechadocumentoAdjudicacion" display-name-key="Fecha documento adjudicación" of-type="DateAndTime.DateOnly" usage="bound" required="false" />
    <property name="descripciondocumentodeadjudicacion" display-name-key="Descripción documento adjudicación" of-type="OptionSet" usage="bound" required="false" />
    <property name="pasosaseguir" display-name-key="Pasos a seguir" of-type="OptionSet" usage="bound" required="false" />
    <property name="fechaInicioProyecto" display-name-key="Fecha inicio proyecto" of-type="DateAndTime.DateOnly" usage="bound" required="false" />
   
    <!-- Fase 11: Firma de contrato -->
    <property name="evaluacionDesempenoTransligra" display-name-key="Evaluación desempeño Transligra" of-type="Whole.None" usage="bound" required="false" />
    <property name="evaluacionDesempenoCliente" display-name-key="Evaluación desempeño cliente" of-type="Whole.None" usage="bound" required="false" />
    <property name="fechaInicioEjecucion" display-name-key="Fecha inicio ejecución" of-type="DateAndTime.DateOnly" usage="bound" required="false" />
    <property name="interlocutorApropiado" display-name-key="Interlocutor apropiado" of-type="Lookup.Simple" usage="bound" required="false" />
    <property name="actaFinalEntrega" display-name-key="Acta final de entrega" of-type="TwoOptions" usage="bound" required="false" />
   
    <resources>
      <code path="index.ts" order="1" />
      <platform-library name="React" version="16.14.0" />
      <platform-library name="Fluent" version="9.46.2" />
    </resources>
    
    <!-- Características necesarias habilitadas -->
    <feature-usage>
      <uses-feature name="Device.pickFile" required="true" />
      <uses-feature name="Utility" required="true" />
      <uses-feature name="WebAPI" required="true" />
    </feature-usage>
  </control>
</manifest>